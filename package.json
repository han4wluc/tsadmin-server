{
   "name": "typeormexpress",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/chai": "^4.2.5",
      "@types/mocha": "^5.2.7",
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^2.12.0",
      "@typescript-eslint/parser": "^2.12.0",
      "chai": "^4.2.0",
      "eslint": "^6.7.2",
      "eslint-config-prettier": "^6.7.0",
      "eslint-plugin-prettier": "^3.1.2",
      "husky": "^3.1.0",
      "mocha": "^6.2.2",
      "nodemon": "^2.0.1",
      "prettier": "^1.19.1",
      "supertest": "^4.0.2",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "body-parser": "^1.18.1",
      "cors": "^2.8.5",
      "express": "^4.15.4",
      "reflect-metadata": "^0.1.10",
      "sqlite3": "^4.0.3",
      "typeorm": "0.2.20"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "lint": "eslint src tests --ext ts",
      "test": "mocha -r ts-node/register tests/**/*.spec.ts",
      "test:watch": "nodemon --exec 'mocha -r ts-node/register tests/**/*.spec.ts'"
